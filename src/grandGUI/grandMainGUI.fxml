<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="620.0" prefWidth="1184.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grandGUI.GrandMainGUIController">
    <AnchorPane VBox.vgrow="ALWAYS">
        <children>
            <Pane layoutY="407.0" prefHeight="220.0" prefWidth="1184.0" style="-fx-background-color: GREEN;">
                <children>
                    <TextField fx:id="nameTextField" layoutX="158.0" layoutY="57.0" />
                    <Label layoutX="14.0" layoutY="56.0" prefHeight="26.0" prefWidth="134.0" text="Set name" textAlignment="CENTER">
                        <font>
                            <Font name="Book Antiqua Italic" size="14.0" />
                        </font>
                    </Label>
                    <TextField fx:id="powerTextField" layoutX="158.0" layoutY="91.0" />
                    <Label layoutX="14.0" layoutY="91.0" prefHeight="26.0" prefWidth="134.0" text="Set power" textAlignment="CENTER">
                        <font>
                            <Font name="Book Antiqua Italic" size="14.0" />
                        </font>
                    </Label>
                    <TextField fx:id="operatingCurrentTextField" layoutX="159.0" layoutY="125.0" />
                    <Label layoutX="14.0" layoutY="125.0" prefHeight="26.0" prefWidth="134.0" text="Set operating current" textAlignment="CENTER">
                        <font>
                            <Font name="Book Antiqua Italic" size="14.0" />
                        </font>
                    </Label>
                    <TextField fx:id="voltageTextField" layoutX="159.0" layoutY="160.0" />
                    <Label layoutX="14.0" layoutY="159.0" prefHeight="26.0" prefWidth="134.0" text="Set voltage" textAlignment="CENTER">
                        <font>
                            <Font name="Book Antiqua Italic" size="14.0" />
                        </font>
                    </Label>
                    <SplitMenuButton fx:id="grandSplitMenuButton" layoutX="158.0" layoutY="14.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="149.0" text="Select type" textAlignment="CENTER">
                        <items>
                            <MenuItem fx:id="pumpMenuItem" mnemonicParsing="false" onAction="#pumpMenuItem" text="Pump" />
                            <MenuItem fx:id="heaterMenuItem" mnemonicParsing="false" onAction="#heaterMenuItem" text="Heater" />
                        </items>
                        <font>
                            <Font size="14.0" />
                        </font>
                    </SplitMenuButton>
                    <Button fx:id="createObject" layoutX="376.0" layoutY="17.0" mnemonicParsing="false" onAction="#createObject" prefHeight="25.0" prefWidth="141.0" text="Create object" />
                    <Button fx:id="nextObject" layoutX="378.0" layoutY="57.0" mnemonicParsing="false" onAction="#nextObject" text="add object to collection" />
                    <Button fx:id="finish" layoutX="378.0" layoutY="91.0" mnemonicParsing="false" onAction="#finish" prefHeight="25.0" prefWidth="141.0" text="Finish" />
                  <TextArea fx:id="grandTextArea" layoutX="592.0" prefHeight="220.0" prefWidth="320.0" />
               <Button fx:id="getPictureButton" layoutX="378.0" layoutY="126.0" mnemonicParsing="false" onAction="#getPicture" prefHeight="25.0" prefWidth="141.0" text="Get picture" />
               <Button fx:id="getSpecsButton" layoutX="378.0" layoutY="160.0" mnemonicParsing="false" onAction="#getSpecs" prefHeight="25.0" prefWidth="141.0" text="Get specs" />
                </children>
            </Pane>
            <Canvas fx:id="canvas" width="1184.0" height="400.0" layoutX="10.0" layoutY="7.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
        </children>
    </AnchorPane>
</VBox>
